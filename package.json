{
  "name": "mocha-notes-plugin",
  "version": "1.0.0",
  "description": "Plugin that monkey-patches the Test object to provide an interface for saving notes during a test",
  "main": "./lib/patch.js",
  "keywords": [
    "mocha",
    "notes"
  ],
  "author": "Quinn Thibeault <quinn.thibeault96@gmail.com>",
  "license": "ISC",
  "peerDependencies": {
    "mocha": "^6.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^6.0.0"
  },
  "scripts": {
    "lint": "eslint lib test",
    "test": "node node_modules/mocha/bin/_mocha --require lib/patch.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinn-thibeault/mocha-notes-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/quinn-thibeault/mocha-notes-plugin/issues"
  },
  "homepage": "https://github.com/quinn-thibeault/mocha-notes-plugin#readme"
}
